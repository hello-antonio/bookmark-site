{
    "name": "bookmark-landing-page",
    "version": "1.0.0",
    "description": "Front end challenge 7",
    "main": "index.js",
    "private": true,
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "prebuild": "rm -rf dist/* docs/*",
        "start": "parcel src/index.html --open",
        "build": "parcel build src/index.html --public-url ./",
        "watch": "parcel watch src/index.html",
        "github:deploy": "npm run build && cp -r dist/* docs && git add . && git commit -a -m 'latest build' && git push origin master"
    },
    "devDependencies": {
        "autoprefixer": "^9.4.9",
        "parcel-bundler": "^1.12.3",
        "postcss-modules": "^1.4.1",
        "sass": "^1.22.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thecalicoder/repo-name.git"
    },
    "author": "Giovanni De Andre <hello.giolara@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/thecalicoder/repo-name/issues"
    },
    "homepage": "https://github.com/thecalicoder/repo-name#readme",
    "dependencies": {
        "parcel": "^1.12.3"
    }
}